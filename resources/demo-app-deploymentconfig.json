{
  "kind": "DeploymentConfig",
  "apiVersion": "v1",
  "metadata": {
    "name": "sti-hello-world"
  },
  "spec": {
    "template": {
      "metadata": {
        "labels": {
          "name": "sti-hello-world"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "HelloWorldDeployment",
            "image": "172.30.67.131:5000/ciao-sti-build/output-sti-test-build",
            "ports": [
              {
                "containerPort": 7777,
                "protocol": "TCP"
              }
            ]
          }
        ]
      }
    },
    "replicas": 2,
    "selector": {
      "name": "sti-hello-world"
    },
    "triggers": [
      {
        "type": "ConfigChange"
      }
    ],
    "strategy": {
      "type": "Rolling"
    }
  }
}